  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/express": "^4.17.21",
    "@types/mocha": "^9.1.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "chai": "^4.3.6",
    "eslint": "^8.56.0",
    "mocha": "^9.2.2",
    "node-typescript": "^0.1.3",
    "nyc": "^15.1.0",
    "register": "^0.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
