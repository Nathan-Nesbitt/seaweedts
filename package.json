{
  "name": "seaweedts",
  "version": "1.0.1",
  "description": "Seaweedfs client library in typescript",
  "main": "seaweed.js",
  "types": "seaweed.d.ts",
  "scripts": {
    "build": "npx tsc --project tsconfig.build.json",
    "coverage": "nyc --check-coverage --lines 60 --per-file --reporter=text npm run test",
